{
  "name": "@vadim-sartakov/react-spreadsheet",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/vadim-sartakov/react-spreadsheet",
  "homepage": "https://github.com/vadim-sartakov/react-spreadsheet#readme",
  "author": "Vadim Sartakov",
  "keywords": [
    "react",
    "excel",
    "spreadsheet",
    "grid"
  ],
  "files": [
    "/lib/**/*"
  ],
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf lib && mkdir lib && cross-env NODE_ENV=production babel src --out-dir lib --ignore src/**/*.stories.js,src/**/*.test.js && copyfiles -u 1 src/**/*.d.ts lib",
    "test": "jest",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "rm -rf docs/storybook && build-storybook -s public -o docs/storybook",
    "build-docs": "rm -rf docs/docs && typedoc --includeDeclarations --excludeExternals --ignoreCompilerErrors --out docs/docs --readme README.md src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "dependencies": {
    "@vadim-sartakov/react-scroller": "vadim-sartakov/react-scroller#build"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/react": "^5.3.14",
    "@types/react": "^16.9.23",
    "babel-jest": "^25.1.0",
    "babel-preset-react-app": "^9.1.1",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "react-scripts": "^3.4.0",
    "typedoc": "^0.16.11"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
